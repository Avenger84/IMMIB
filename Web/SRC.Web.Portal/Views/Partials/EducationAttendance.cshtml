@using SRC.Library.Entities.CrmEntities
@model DynamicPage

<section id="main-content" style="width:80%;margin:auto;">
    <section class="wrapper">
        <!-- page start-->
        <div class="row">
            <div class="col-lg-12">
                <section class="panel" style="padding:20px;">
                    <div class="panel-body">
                        @Html.Raw(Model.Content)
                    </div>
                </section>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="checkbox">
                    <label>
                        <input type="checkbox" value="" id="chkAttendance">
                        Eğitim ve başvuru koşullarını kabul ediyorum
                    </label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <a class="btn btn-primary" href="#" id="@ViewBag.Id" role="button">Eğitime Başvur</a>
            </div>
        </div>
        <!-- page end-->
    </section>
</section>
